@page
@model animalShelter.Pages.CatAdoptions.DetailsModel

@{
    ViewData["Title"] = "Cat Adoption Details";
}
<br/>
<div>
    <h4>Cat Adoption</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CatAdoption.Cat)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CatAdoption.Cat.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CatAdoption.User)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CatAdoption.User.FullName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CatAdoption.AdoptionDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CatAdoption.AdoptionDate)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.CatAdoption.CatAdoptionID">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
