    ViewData["Title"] = "Cat Details";
}
<br/>
<div>
    <h4>Cat Details</h4>
    <hr/>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.Dob)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.Dob)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.Breed)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.Breed)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.Sex)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.Sex)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.Summary)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.Summary)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cat.MainImage)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cat.MainImage)
        </dd>
